{
  "name": "Simple Admin (React Build)",
  "description": "This is an extension designed to make searching the SCCRM database more intuitive.",
  "version": "1",
  "manifest_version": 2,
  "permissions": ["storage", "tabs", "activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
